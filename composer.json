{
    "name": "rcalicdan/mysql-binary-protocol",
    "description": "Mysql Binary Protocol for my fiber async libary credits to Ivan Chepurnyi this a modernise implementation of his works",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Reymart A. Calicdan",
            "email": "reymart.calicdan06@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.3" 
    },
    "autoload": {
        "psr-4": {
            "Rcalicdan\\MySQLBinaryProtocol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EcomDev\\MySQLBinaryProtocol\\": "tests/"
        }
    },
    "require-dev": {
        "pestphp/pest": "^4.0"
    },
    "scripts": {
        "test": "vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}